{
    "manifest_version": 2,
    "name": "YouTube_AdSkip",
    "version": "1.4",
    "content_scripts": [
        {
            "matches": [
                "https://www.youtube.com/*"
            ],
            "js": [
                "myscript.js"
            ],
            "run_at": "document_idle"
        }
    ],
    "web_accessible_resources": [
        "cancel-volume-disabling.js"
    ],
    "icons": {
        "16": "resources/icon16.png",
        "48": "resources/icon48.png",
        "96": "resources/icon96.png",
        "128": "resources/icon128.png"
    }
}
